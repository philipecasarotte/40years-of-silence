<h1>Listing Photos</h1>

<table>
  <tr>
    <th>Title</th>
  </tr>
  <%- @photos.each do |photo|%>
    <tr>
      <td><%= image_tag(photo.image.url(:thumb)) %></td>
      <td><%=link_to 'Edit', edit_object_url(photo) %></td>
      <td><%=link_to 'Destroy', object_url(photo), :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>
<br/>

<%= link_to 'New Photo', new_object_url %> | <%= link_to "Reorder", reorder_admin_album_photos_path(parent_object) %> | <%= link_to "Back", admin_albums_path %>