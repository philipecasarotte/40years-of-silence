<h1>New Download</h1>

<%= error_messages_for :download %>

<% form_for(:download, :url => collection_url, :html=>{ :multipart => true }) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>
<br/>
<%= link_to 'Back', collection_url %>