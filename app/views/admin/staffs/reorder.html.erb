<h1>Reorder</h1>

<ul id="order">
	<% @items.each do |staff| %>
	<li id="item_<%=staff.id%>"><%= staff.name %></li>
	<% end %>
</ul>

<%= sortable_element 'order', 
      :complete => visual_effect(:highlight, 'order'), 
      :url => { :action => "order" } %>

<p><%= link_to "Back", admin_staffs_path %></p>