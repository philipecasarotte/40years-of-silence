<%= render(:partial => 'layouts/meta_data', :locals => {:obj => @page}) if @page %>
<% @title = @page.title %>
<div id="content">
	<h1><%= h @page.title %></h1>
	<div class="content_cms">
		<%= @page.body %>
	</div>
</div>
<% begin %>
	<% content_for :head do -%>
		<%= javascript_tag "$(function(){set_menu('#{@page.parents[0].permalink}');});" %>
	<% end -%>
<% rescue %>
	<% content_for :head do -%>
		<%= javascript_tag "$(function(){set_menu('#{@page.permalink}');});" %>
	<% end rescue "" -%>
<% end %>