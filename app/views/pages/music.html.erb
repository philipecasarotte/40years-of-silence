<%= render(:partial => 'layouts/meta_data', :locals => {:obj => @page}) if @page %>
<% @title = @page.title %>
<div id="sub_nav">
	<div id="player"></div>
</div>
<div id="content">
	<h1><%= @page.title %></h1>
	<div class="content_cms">
		<%= @page.body %>
	</div>
</div>
<% content_for :head do -%>
	<%= javascript_tag "$(function(){set_menu('film');});" %>
	<%= javascript_include_tag "page/music" %>
<% end -%>