<%= render(:partial => 'layouts/meta_data', :locals => {:obj => @page}) if @page %>
<% @title = @page.title %>
<div id="content" class="directors_statements">
	<h1><%= @page.title %></h1>
	<div id="videoPlayer">&nbsp;</div>
	<div class="content_cms"><%= @page.body %></div>
</div>
<% content_for :head do -%>
	<% javascript_tag do -%>
		$(function(){
			$("#menu_crew").addClass("selected");
		});
	<% end -%>
	<%= javascript_include_tag "plugins/flashembed.min", "page/directors_statements" %>
<% end -%>
